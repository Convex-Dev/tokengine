# /etc/systemd/systems/tokengine.service
#
[Unit]
Description=Tokengine Daemon
Requires=network.target
After=network.target
   
[Service]    
Type=simple
WorkingDirectory={{ tokengine_homedir }}
User={{ tokengine_user }}
Group={{ tokengine_group }}
ExecStart=/usr/bin/java -jar {{ tokengine_homedir }}/{{ tokengine_package }}
#TimeoutSec=30
RestartSec=10
Restart=on-failure
   
[Install]
WantedBy=default.target
##

